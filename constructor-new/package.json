{
  "name": "calendar-bio-constructor",
  "version": "1.0.0",
  "description": "Fetches events, combines them and stores them in the Google calendar",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --watch 'src/**' --ext 'js,json' --exec 'node src/index.js -d'",
    "format": "prettier --write '{src,tests}/**/*.{js,html,json}'",
    "test": "ava",
    "test:watch": "ava --watch",
    "snapshots": "ava -u"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BirminghamIO/calendar"
  },
  "author": "Stuart Langridge <sil@kryogenix.org>",
  "contributors": [
    "James Vickery <dev@jmsv.me>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/BirminghamIO/calendar/issues"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.10",
    "commander": "^8.1.0",
    "consola": "^2.15.3",
    "handlebars": "^4.7.7",
    "ical-generator": "^3.0.0",
    "moment": "^2.29.1",
    "puppeteer": "^10.2.0",
    "query-string": "^7.0.1"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "sinon": "^11.1.2"
  }
}
